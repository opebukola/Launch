<% content_for :head do %>
  <%= javascript_include_tag 'http://www.google.com/jsapi' %>
<% end %>
<h2>All Users</h2>
<div class="span9">
  <table class="datatable table table-bordered table-condensed">
    <thead>
      <tr>
        <th>Email</th>
        <th>Requested</th>
        <th>Invitation</th>
      </tr>
    </thead>
    <tbody>
      <% @users.each do |user| %>
        <tr>
          <td><%= link_to user.email, user %></td>
          <td><%= user.created_at.to_date %></td>
          <td><%= link_to "send invitation", '#'  %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>